UnicodeData = record++LF, LF .
record = codepoint, -';', name, -';', category, -';', combining, -';',
         bidi, -';', decomposition, -';', decimal, -';', digit, -';',
         numeric, -';', mirrored, -';', name_1_0, -';', comment, -';',
         uppercase, -';', lowercase, -';', titlecase .

-LF = -#a.

-value = ~[';']* .
codepoint = ["0"-"9";"A"-"F"]+ .
name = ~[';']+ .
category = value .
combining = value .
bidi = value .
decomposition = value .
decimal = value .
digit = value .
numeric = value .
mirrored = value .
name_1_0 = value .
comment = value .
uppercase = value .
lowercase = value .
titlecase = value .
